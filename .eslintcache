 from 'react'\nimport { Header, Divider, Input, Button, Form } from 'semantic-ui-react'\nimport { useForm, Controller } from 'react-hook-form'\nimport { login } from './actions'\n\nfunction Login({ history }) {\n    const { handleSubmit, control } = useForm()\n    const [loading, setLoading] = React.useState()\n    const onSubmitHandler = async values => {\n        setLoading(true)\n        try {\n            const { data } = await login(values)\n            localStorage.setItem('user', data.data.token)\n            window.location.reload(false)\n        } catch (e) {\n            console.log(e)\n        }\n        setLoading(false)\n    }\n\n    return (\n        <React.Fragment>\n            <Header inverted as=\"h2\">\n                Sign In\n            </Header>\n            <Form onSubmit={handleSubmit(onSubmitHandler)}>\n                <Controller\n                    control={control}\n                    name=\"email\"\n                    defaultValue={'admin@knit.fin'}\n                    rules={{ required: true }}\n                    render={({ onChange, onBlur, value, ref }) => (\n                        <Form.Field>\n                            <Input\n                                size=\"huge\"\n                                placeholder=\"Email address\"\n                                onChange={onChange}\n                                value={value}\n                            />\n                        </Form.Field>\n                    )}\n                />\n                <Controller\n                    control={control}\n                    name=\"password\"\n                    defaultValue={'123456789'}\n                    rules={{ required: true }}\n                    render={({ onChange, onBlur, value, ref }) => (\n                        <Form.Field>\n                            <Input\n                                size=\"huge\"\n                                type=\"password\"\n                                placeholder=\"Password\"\n                                onChange={onChange}\n                                value={value}\n                            />\n                        </Form.Field>\n                    )}\n                />\n\n                <Form.Field>\n                    <Button primary size=\"huge\" loading={loading}>\n                        Sign In\n                    </Button>\n                </Form.Field>\n            </Form>\n        </React.Fragment>\n    )\n}\n\nexport default Login\n",["67","68"],"/Users/sibizulu/Code/dev/knit/knitapp/src/Withdraw.js",[],"/Users/sibizulu/Code/dev/knit/knitapp/src/Withdrawals.js",["69","70","71"],"import * as React from 'react'\nimport { Header, Icon, Table, Label } from 'semantic-ui-react'\nimport { getWithdraw } from './actions'\n\nconst truncate = (str, max, sep) => {\n    max = max || 6\n\n    var len = str.length\n    if (len > max) {\n        sep = sep || '...'\n        var seplen = sep.length\n        if (seplen > max) {\n            return str.substr(len - max)\n        }\n        var n = -0.5 * (max - len - seplen)\n        var center = len / 2\n\n        var front = str.substr(0, center - n)\n        var back = str.substr(len - center + n)\n        return front + sep + back\n    }\n\n    return str\n}\n\nfunction Withdrawals() {\n    const [items, setItems] = React.useState([])\n\n    React.useEffect(() => {\n        getWithdraw().then(res => {\n            setItems(res.data.data.result)\n        })\n    }, [])\n    console.log(items)\n\n    const listItems = items.map((item, index) => (\n        <Table.Row key={index} inverted>\n            <Table.Cell collapsing>\n                {truncate(item.walletTo, 16, '...')}\n            </Table.Cell>\n            <Table.Cell>{item.createdAt}</Table.Cell>\n            <Table.Cell collapsing textAlign=\"right\">\n                {item.amount}\n            </Table.Cell>\n            <Table.Cell collapsing textAlign=\"right\">\n                {item.coin}\n            </Table.Cell>\n            <Table.Cell collapsing textAlign=\"right\">\n                {`${item.amount} k${item.coin}`}\n            </Table.Cell>\n            <Table.Cell collapsing>\n                <a\n                    target=\"_blank\"\n                    href={`https://kovan.etherscan.io/tx/${item.txnId}`}>\n                    {truncate(item.txnId, 8, '...')}\n                </a>\n            </Table.Cell>\n\n            <Table.Cell collapsing>\n                {item.status && (\n                    <Label size=\"mini\" color=\"green\">\n                        Confirm\n                    </Label>\n                )}\n            </Table.Cell>\n        </Table.Row>\n    ))\n    return (\n        <React.Fragment>\n            <Table celled striped>\n                <Table.Header>\n                    <Table.Row>\n                        <Table.HeaderCell colSpan=\"7\">\n                            Withdrawals\n                        </Table.HeaderCell>\n                    </Table.Row>\n                    <Table.Row>\n                        <Table.HeaderCell>Receiving Wallet</Table.HeaderCell>\n                        <Table.HeaderCell>Date</Table.HeaderCell>\n                        <Table.HeaderCell>Amount</Table.HeaderCell>\n                        <Table.HeaderCell>Coin</Table.HeaderCell>\n                        <Table.HeaderCell>Tokens</Table.HeaderCell>\n                        <Table.HeaderCell>Explorer</Table.HeaderCell>\n                        <Table.HeaderCell>Status</Table.HeaderCell>\n                    </Table.Row>\n                </Table.Header>\n\n                <Table.Body>\n                    {listItems}\n                    {items.length === 0 && (\n                        <Table.Row>\n                            <Table.Cell>No withdrawals yet!</Table.Cell>\n                        </Table.Row>\n                    )}\n                </Table.Body>\n            </Table>\n        </React.Fragment>\n    )\n}\n\nexport defa